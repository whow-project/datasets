# #LogicalSourceAcqueSotterranee
<#LogicalSourceAcqueSotterranee> a rml:BaseSource ;
    rml:source "Allegato_2_DATI_PFAS_2018_ACQUE_SOT_4_.csv";
    rml:referenceFormulation ql:CSV .



###########################################################
##                  MAPPING RULES
##########################################################


# #################################
# ## GROUNDWATER WATER BODY  MAPPING:
# #################################
<#GroundwaterWaterBodyMapping> a rr:TriplesMap ;
rml:logicalSource <#LogicalSourceAcqueSotterranee> ;
rr:subjectMap [
  rr:template "https://w3id.org/whow/data/groundwaterwaterbody/{GWB}";
  rr:termType rr:IRI;
  rr:class hydro:GroundWaterBody , hydro:WaterBody , hydro:WaterFeature , ispra-emf:FeatureOfInterest
] ;
rr:predicateObjectMap [
  rr:predicate rdfs:label ;
  rr:objectMap [ rr:template "Ground water body {GWB}" ; rr:language "en" ]
];
rr:predicateObjectMap [
  rr:predicate rdfs:label ;
  rr:objectMap [ rr:template "Corso d'acqua sotterraneo {GWB}" ; rr:language "it" ]
];
rr:predicateObjectMap [
  rr:predicate ispra-top:name ;
  rr:objectMap [ rr:template "{GWB}" ; rr:language "it" ]
];
rr:predicateObjectMap [
  rr:predicate l0:name ;
  rr:objectMap [ rr:template "{GWB}" ; rr:language "it" ]
];
rr:predicateObjectMap [
  rr:predicate ispra-top:identifier ;
  rr:objectMap [ rml:reference "GWB" ]
];
rr:predicateObjectMap [
  rr:predicate l0:identifier ;
  rr:objectMap [ rml:reference "GWB" ]
].
