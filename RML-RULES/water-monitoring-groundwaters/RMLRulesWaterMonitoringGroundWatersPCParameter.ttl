<#OtherMappingForGW>
rr:termType rr:IRI ;
fnml:functionValue [
rr:predicateObjectMap [
   rr:predicate fno:executes ;
   rr:objectMap [ rr:constant grel:array_join ]
 ] ;
 rr:predicateObjectMap [
   rr:predicate grel:p_array_a ;
   rr:objectMap  [ rr:constant "https://w3id.org/italia/lombardia/data/observable-properties"]
 ];
 rr:predicateObjectMap [
