fnml:functionValue [
  rr:predicateObjectMap [
    rr:predicate fno:executes ;
    rr:objectMap [ rr:constant grel:controls_if ]
  ];
  rr:predicateObjectMap [
    rr:predicate grel:bool_b ;
    rr:objectMap <#80EPiu>
  ];
  rr:predicateObjectMap [
    rr:predicate grel:any_true ;
    rr:objectMap <#Slugify80>
  ] ;
