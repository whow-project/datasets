@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix grel: <http://users.ugent.be/~bjdmeest/function/grel.ttl#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix idlab-fn: <http://example.com/idlab/function/> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix top: <https://dati.isprambiente.it/ontology/top/> .
@prefix w-mon: <https://w3id.org/whow/onto/water-monitoring/>.
@base         <https://w3id.org/whow/controlled-vocabularies/observable-properties/> .

##java -jar .\rmlmapper-4.15.0-r361-all.jar -m .\test-mapping-observable-properties-wise-substances.ttl -o output-wise-observable-properties-mapping.ttl -s turtle

##########################################################
## LOGICAL SOURCES TO BE USED
##########################################################

# #LogicalSourceSostanzeChimiche
<#LogicalSourceWISECodes> a rml:BaseSource ;
    rml:source [
      a csvw:Table;
      #csvw:url "Monitoraggio_laghi_dati_puliti.csv";
      csvw:url "wise-codes-dataset.csv";

      csvw:dialect [
        a csvw:Dialect;
        csvw:delimiter ","
      ]
    ];
    rml:referenceFormulation ql:CSV .

############# CSMapping ###########


<#WISECodesMapping> a rr:TriplesMap ;
rml:logicalSource <#LogicalSourceWISECodes> ;
rr:subjectMap [
  rr:template "https://w3id.org/whow/controlled-vocabularies/observable-properties/wise-{WISE}";
  rr:termType rr:IRI;
  rr:class w-mon:WaterObservableProperty;
  rr:class skos:Concept
] ;
rr:predicateObjectMap [
  rr:predicate rdfs:label;
  rr:objectMap [ rr:template "{PARAMETER (IT)}- Codice WISE: {WISE}" ; rr:language "it" ]
];
rr:predicateObjectMap [
  rr:predicate skos:prefLabel;
  rr:objectMap [ rr:template "{PARAMETER (IT)}" ; rr:language "it" ]
];
rr:predicateObjectMap [
  rr:predicate top:hasUniqueIdentifier;
  rr:objectMap [   rr:template "https://w3id.org/whow/data/identifier/{WISE}"; rr:termType rr:IRI;]
];
rr:predicateObjectMap [
  rr:predicate skos:notation;
  rr:objectMap [  rr:template "WISE:{WISE}" ]
].



<#UniqueWISEIDMapping> a rr:TriplesMap ;
rml:logicalSource <#LogicalSourceWISECodes> ;
rr:subjectMap [
  rr:template "https://w3id.org/whow/data/identifier/{WISE}";
  rr:termType rr:IRI;
  rr:class top:UniqueIdentifier
] ;
rr:predicateObjectMap [
  rr:predicate top:identifier;
  rr:objectMap [ rr:template "{WISE}"; rr:datatype xsd:string ]
];
rr:predicateObjectMap [
  rr:predicate rdfs:label;
  rr:objectMap [ rr:template "Identificativo WISE — {WISE}" ; rr:language "it" ]
];
rr:predicateObjectMap [
  rr:predicate rdfs:label;
  rr:objectMap [ rr:template "WISE Identifier — {WISE}" ; rr:language "en" ]
];
rr:predicateObjectMap [
  rr:predicate top:isIssuedBy;
  rr:objectMap [ rr:template "https://w3id.org/whow/data/organisation/european-environmental-agency"; rr:termType rr:IRI; ]
].
