##########################################################
## LOGICAL SOURCES TO BE USED
##########################################################

# #LogicalSourceSostanzeChimiche
<#LogicalSourceWISECodes> a rml:BaseSource ;
    rml:source [
      a csvw:Table;
      #csvw:url "Monitoraggio_laghi_dati_puliti.csv";
      csvw:url "observable-properties-without-cas-dataset.csv";

      csvw:dialect [
        a csvw:Dialect;
        csvw:delimiter ","
      ]
    ];
    rml:referenceFormulation ql:CSV .

############# CSMapping ###########

<#WISECodesMapping> a rr:TriplesMap ;
rml:logicalSource <#LogicalSourceWISECodes> ;
rr:subjectMap [
  rr:template "https://w3id.org/whow/controlled-vocabularies/observable-properties/{CAS}";
  rr:termType rr:IRI;
  rr:class w-mon:WaterObservableProperty;
  rr:class skos:Concept
] ;
rr:predicateObjectMap [
  rr:predicate rdfs:label;
  rr:objectMap [ rr:template "Concentrazione di {SOSTANZA}" ; rr:language "it" ]
];
rr:predicateObjectMap [
  rr:predicate ispra-top:hasUniqueIdentifier;
  rr:objectMap [   rr:template "https://w3id.org/whow/data/identifier/{CAS}"; rr:termType rr:IRI;]
];
rr:predicateObjectMap [
  rr:predicate skos:prefLabel;
  rr:objectMap [ rr:template "{SOSTANZA}" ; rr:language "it" ]
].

<#UniqueWISEIDMapping> a rr:TriplesMap ;
rml:logicalSource <#LogicalSourceWISECodes> ;
rr:subjectMap [
  rr:template "https://w3id.org/whow/data/identifier/{CAS}";
  rr:termType rr:IRI;
  rr:class ispra-top:UniqueIdentifier
] ;
rr:predicateObjectMap [
  rr:predicate ispra-top:identifier;
  rr:objectMap [ rr:template "{CAS}"; rr:datatype xsd:string ]
];
rr:predicateObjectMap [
  rr:predicate rdfs:label;
  rr:objectMap [ rr:template "Identificativo {CAS}." ; rr:language "it" ]
];
rr:predicateObjectMap [
  rr:predicate rdfs:label;
  rr:objectMap [ rr:template "Identifier {CAS}" ; rr:language "en" ]
].
