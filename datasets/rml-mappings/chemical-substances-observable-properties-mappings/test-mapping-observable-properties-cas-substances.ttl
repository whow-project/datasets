##########################################################
## LOGICAL SOURCES TO BE USED
##########################################################

# #LogicalSourceSostanzeChimiche
<#LogicalSourceConcentrationOfChemicalSubstances> a rml:BaseSource ;
    rml:source [
      a csvw:Table;
      #csvw:url "Monitoraggio_laghi_dati_puliti.csv";
      csvw:url "chemical-substances-complete.csv";

      csvw:dialect [
        a csvw:Dialect;
        csvw:delimiter ","
      ]
    ];
    rml:referenceFormulation ql:CSV .

############# CSMapping ###########


<#ChemicalSubstancesConcentrationMapping> a rr:TriplesMap ;
rml:logicalSource <#LogicalSourceConcentrationOfChemicalSubstances> ;
rr:subjectMap [
  rr:template "https://w3id.org/whow/controlled-vocabulary/observable-properties/{CAS}";
  rr:termType rr:IRI;
  rr:class w-mon:WaterObservableProperty;
  rr:class skos:Concept
] ;
rr:predicateObjectMap [
  rr:predicate rdfs:label;
  rr:objectMap [ rr:template "Concentrazione di {PARAMETER(IT)}" ; rr:language "it" ]
];
rr:predicateObjectMap [
  rr:predicate skos:prefLabel;
  rr:objectMap [ rr:template "Concentration of {EC Name}" ; rr:language "en"]
];
rr:predicateObjectMap [
  rr:predicate skos:prefLabel;
  rr:objectMap [ rr:template "Concentrazione di {EC Name}" ; rr:language "it"]
];
rr:predicateObjectMap [
  rr:predicate w-mon:hasChemicalSubstance;
  rr:objectMap [ rr:template "https://w3id.org/whow/controlled-vocabulary/chemical-substances/cas-{CAS}" ; rr:termType rr:IRI;]
].
