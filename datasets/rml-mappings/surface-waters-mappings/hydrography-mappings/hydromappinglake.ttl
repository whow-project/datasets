##################
## WATER BASIN
##################
<#WaterBasinMapping> a rr:TriplesMap ;
rml:logicalSource <#LogicalSourceLaghi> ;
rr:subjectMap [
  fnml:functionValue [
    rr:predicateObjectMap [
     rr:predicate fno:executes ;
     rr:objectMap [ rr:constant grel:array_join ]
    ] ;
    rr:predicateObjectMap [
     rr:predicate grel:p_array_a ;
     rr:objectMap  [ rr:constant "https://w3id.org/whow/data/water-basin"]
    ];
    rr:predicateObjectMap [
       rr:predicate grel:p_array_a ;
       rr:objectMap <#RemoveSpace>
    ];
    rr:predicateObjectMap [
       rr:predicate grel:p_string_sep ;
       rr:objectMap [ rr:constant "/" ]
    ];
 ];
rr:class hydro:WaterBasin , hydro:WaterFeature , ispra-mf:FeatureOfInterest
] ;
rr:predicateObjectMap [
  rr:predicate top:name ;
  rr:objectMap [ rr:template "{BACINO IDROGRAFICO}" ; rr:language "it" ]
];
rr:predicateObjectMap [
  rr:predicate l0:name ;
  rr:objectMap [ rr:template "{BACINO IDROGRAFICO}" ; rr:language "it" ]
];
rr:predicateObjectMap [
  rr:predicate rdfs:label;
  rr:objectMap [ rr:template "Water basin {BACINO IDROGRAFICO}" ; rr:language "en" ]
];
rr:predicateObjectMap [
  rr:predicate rdfs:label;
  rr:objectMap [ rr:template "Bacino idrografico {BACINO IDROGRAFICO}" ; rr:language "it" ]
].
